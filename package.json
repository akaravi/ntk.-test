{
  "name": "ntk-cms-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "serve1": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve",
    "serve2": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve",
    "serve3": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng serve",
    "serve4": "node --max_old_space_size=32768 ./node_modules/@angular/cli/bin/ng serve",
    "build-prod": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production --build-optimizer",
    "build-prod2": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer",
    "build-prod3": "ng build --prod",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ntk-cms-web/stats.json",
    "start-pwa": "ng build --configuration production && http-server -p 8080 -c-1 dist/ntk-cms-web",
    "serve:ssr:ntk-cms-web": "node dist/ntk-cms-web/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@ali-hm/angular-tree-component": "^12.0.5",
