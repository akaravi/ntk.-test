    "@asymmetrik/ngx-leaflet": "^18.0.1",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@microsoft/signalr": "^7.0.14",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.8",
    "apexcharts": "^3.49.1",
    "firebase": "^10.12.2",
